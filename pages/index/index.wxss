.index_swiper swiper {
  width: 650rpx;
  height: 275rpx;
  margin-left: 50rpx;
}
.index_swiper {
   margin-top: -140rpx;
}
.index-con {
    background: #67c9a8;
    height: 160rpx;
    border-radius: 0 0 100rpx 100rpx;
}
.index_swiper swiper swiper-item image {
  width: 100%;
  border-radius: 30rpx;
  height: 250rpx !important;
}
.index_bigType {
  padding-top: 30rpx;
  background-color: #FFFFFF;
}
.index_bigType .bigTypeRow {
  display: flex;
}
.index_bigType .bigTypeRow navigator {
  flex: 1;
  
}
.index_hotProduct {
    background: #fff !important;
}
.index_bigType .bigTypeRow navigator image {
  width: 120rpx;
  margin-left: 30rpx;
}
.index_bigType .bigTypeRow navigator .nav-name {
  text-align: center;
  font-size: 12px;
  font-style: normal;
}
.index_hotProduct .product_title {
  font-size: 32rpx;
  font-weight: 600;
  padding: 20rpx;
  margin-top: 20rpx;
  color: #67c9a8;
}
.index_hotProduct .product_list {
      display: flex;
    white-space: nowrap;
    flex-direction: row;
    width: 100%;
    align-items: center;
}
::-webkit-scrollbar{ width: 0; height: 0; color: transparent;}
.index_hotProduct .product_list .product_detail {
  margin: 15rpx;
  width: 400rpx;
  text-align: center;
  background-color: #FFFFFF;
  display: inline-block;
}
.index_hotProduct .product_list .product_detail navigator image {
  width: 100%;
  height: 400rpx;
  border-radius: 10rpx;
}
.index_hotProduct .product_list .product_detail navigator .product_name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.index_hotProduct .product_list .product_detail navigator .product_price {
  color: #67c9a8;
}

.product_tip {
    font-size: 22rpx;
    color: #bdb9b9;
    text-overflow: -o-ellipsis-lastline;
	  overflow: hidden;
	  text-overflow: ellipsis;
	  display: -webkit-box;
	  -webkit-line-clamp: 2;
	  -webkit-box-orient: vertical;
}

